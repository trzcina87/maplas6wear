            android:maxHeight="106px"
            android:minHeight="106px"
            android:maxWidth="106px"
            android:minWidth="106px"
            android:adjustViewBounds="true"
            android:tag="Poziomka"
            android:src="@mipmap/poziomka" />

        <ImageView
            android:id="@+id/im9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxHeight="106px"
            android:minHeight="106px"
            android:maxWidth="106px"
            android:minWidth="106px"
            android:adjustViewBounds="true"
            android:tag="Punkt"
            android:src="@mipmap/znacznik2" />

    </LinearLayout>

</LinearLayout>