<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_gravity="center_vertical|center_horizontal"
    android:gravity="center_vertical|center_horizontal"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|center_horizontal"
        android:gravity="center_vertical|center_horizontal"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/im1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxHeight="106px"
            android:minHeight="106px"
            android:maxWidth="106px"
            android:minWidth="106px"
            android:adjustViewBounds="true"
            android:tag="Prawdziwek"
            android:src="@mipmap/prawdziwek" />

        <ImageView
            android:id="@+id/im2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxHeight="106px"
            android:minHeight="106px"
            android:maxWidth="106px"
            android:minWidth="106px"
            android:adjustViewBounds="true"
            android:tag="Kurka"
            android:src="@mipmap/kurka" />

        <ImageView
            android:id="@+id/im3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxHeight="106px"
            android:minHeight="106px"
            android:maxWidth="106px"
            android:minWidth="106px"
            android:adjustViewBounds="true"
            android:tag="Piaskowiec"
            android:src="@mipmap/piaskowiec" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|center_horizontal"
        android:gravity="center_vertical|center_horizontal"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/im4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxHeight="106px"
            android:maxWidth="106px"
            android:minHeight="106px"
            android:minWidth="106px"
            android:adjustViewBounds="true"
            android:tag="Atrakcja"
            android:src="@mipmap/atrakcja" />

        <ImageView
            android:id="@+id/im5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxHeight="106px"
            android:minHeight="106px"
            android:maxWidth="106px"
            android:minWidth="106px"
            android:adjustViewBounds="true"
            android:tag="Parking"
            android:src="@mipmap/parking" />

